{
  "expo": {
    "name": "James Avisa",
    "slug": "porteiro-app",
    "version": "1.0.0",
    "scheme": "porteiro-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "color": "#4CAF50",
          "defaultChannel": "default",
          "sounds": [
            "./assets/notification.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/logo-james-fundo.png",
    "userInterfaceStyle": "light",
    "primaryColor": "#4CAF50",
    "backgroundColor": "#F9FAFB",
    "theme": {
      "primary": "#4CAF50",
      "secondary": "#2196F3",
      "accent": "#FF9800",
      "error": "#F44336",
      "warning": "#FF9800",
      "info": "#2196F3",
      "success": "#4CAF50",
      "surface": "#FFFFFF",
      "background": "#F9FAFB",
      "onPrimary": "#FFFFFF",
      "onSecondary": "#FFFFFF",
      "onSurface": "#333333",
      "onBackground": "#333333"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.porteiroapp.notifications",
      "buildNumber": "1.0.0",
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#4CAF50"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "package": "com.porteiroapp.notifications",
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "statusBar": {
        "backgroundColor": "#388E3C",
        "barStyle": "light-content"
      },
      "navigationBar": {
        "backgroundColor": "#4CAF50"
      }
    },
    "notification": {
      "color": "#4CAF50",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "James Avisa"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "74e123bc-f565-44ba-92f0-86fc00cbe0b1"
      }
    }
  }
}
