{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "apk": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "ios-prod": {
      "distribution": "store",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
